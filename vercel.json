{
  "version": 2,
  "functions": {
    "api/lightweight.py": {
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/health",
      "dest": "/api/lightweight.py"
    },
    {
      "src": "/recognize",
      "dest": "/api/lightweight.py"
    },
    {
      "src": "/(.*)",
      "dest": "/api/lightweight.py"
    }
  ]
}
